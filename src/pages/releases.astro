---
import ReleaseDocCard from "../components/ReleaseDocCard.astro";
import Layout from "../layouts/Layout.astro";
import { getLatestLaunches } from "../services/spacex";

const data = await getLatestLaunches({ limit: 10 });
---

<Layout title="Releases">
  <div class="flex flex-col gap-4">
    {data.docs.map((doc) => <ReleaseDocCard doc={doc} />)}
  </div>
</Layout>
